@page "/overlay"

@inject iRacingService IRacingService
@layout Overlay

<StateHeader Visible="true"></StateHeader>

<Standings Visible="true"></Standings>

<Logo Visible="true"></Logo>

<DriverInfo Visible="true" Photo="false"></DriverInfo>

<Twitch></Twitch>


@code
{
    protected override void OnInitialized()
    {
        IRacingService.StateChanged += StateHasChanged;
        base.OnInitialized();
    }
}
