# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = tab
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
max_line_length = 130
indent_size = 4

[*.json]
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = false
